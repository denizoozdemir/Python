a = int(input("ikinci dereceden denklemin a degerini giriniz: "))
b = int(input("ikinci dereceden denklemin b degerini giriniz: "))
c = int(input("ikinci dereceden denklemin c degerini giriniz: "))
import math
delta = b**2-(4*a*c)
print("Deltası: ",delta)
if delta>0:
    kok1 = (-b + math.sqrt(delta))/2*a
    kok2 = (-b - math.sqrt(delta))/2*a
    print("Sistemin iki kökü vardır",kok1, "...",kok2)
elif delta==0:
    kok = -b/(2*a)
    print("çakışık kök ",kok)
else: print("sistemin kökü yoktur")
